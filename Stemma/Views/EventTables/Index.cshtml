@model IEnumerable<Stemma.Models.EventTable>

@{
    ViewBag.Title = "Your TimeLine";
}
<script src="https://static.filestackapi.com/v3/filestack.js"></script>
<script src="~/Scripts/fileUploader.js"></script>
<link href="~/Content/TimeLine.css" rel="stylesheet" />
<h2>Your Stemma Timeline</h2>

<section class="timeline">
    <ul>
        @foreach (var item in Model)
        {

            <li>
                <div>
                    <h1><b>@Html.DisplayFor(modelItem => item.EventTitle)</b></h1>


                    <img class="uploadedImage" src="@item.UploadMedia" alt="DanaPic" id="itemImg" />                  


                    <time>@Html.DisplayFor(modelItem => item.EventYear)</time>
                    <p>@Html.DisplayFor(modelItem => item.EventCaption)</p>




                    @*@Html.ActionLink("Edit", "Edit", new { id = item.EventID }) |*@
                    @*@Html.ActionLink("Details", "Details", new { id = item.EventID }) |*@
                    @*@Html.ActionLink("Delete", "Delete", new { id = item.EventID })*@
                </div>
            </li>
        }



    </ul>
</section>


